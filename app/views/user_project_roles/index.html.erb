<h3>User Project Roles</h3>

<table>
  <thead>
  <tr>
          <th>User</th>
          <th>Project</th>
          <th>Role</th>
        <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @user_project_roles.each do |user_project_role| %>
  <tr>
          <td><%= user_project_role.user_id %></td>
          <td><%= user_project_role.project_id %></td>
          <td><%= user_project_role.role %></td>
        <td><%= link_to 'Show', user_project_role %></td>
    <td><%= link_to 'Edit', edit_user_project_role_path(user_project_role) %></td>
    <td><%= link_to 'Destroy', user_project_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Project Role', new_user_project_role_path %>